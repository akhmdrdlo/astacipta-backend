      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "namaProgram"
    },
    "PICProgram": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "gambarUtama": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Mitra": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "kerjasama.mitra",
      "repeatable": true,
      "required": true
    },
    "deskripsiProgram": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "cakupanProgram": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Roadmap": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "program.roadmap",
      "repeatable": true
    },
    "prediksiAnggaran": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sumberDana": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "MitraPotensial": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }

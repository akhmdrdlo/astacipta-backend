{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "namaProgram": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true

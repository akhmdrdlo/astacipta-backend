    "progressTindakLanjut": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "risikoDanHambatan": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "dokumenTerkait": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "produks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::produk.produk",
      "mappedBy": "program"
    }
  }
